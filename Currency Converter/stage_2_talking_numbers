def greet_new_crypto():
    print("Meet a conicoin!")

def convert_to_usd(num:int) -> None:
    conversion = 100
    print(f"I have {num} conicoins.\n{num} conicoins cost {num * conversion} dollars.\nI am rich! Yippee!")

if __name__ == '__main__':
    # greet_new_crypto()
    try:
        conis = int(input())
        convert_to_usd(conis)
    except ValueError:
        print("Input has to be a number")